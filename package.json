    "jest-svg-transformer": "^1.0.0",
    "json-server": "^0.17.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.1",
    "prettier": "^2.7.1",
