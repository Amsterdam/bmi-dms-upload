{
  "name": "@amsterdam/bmi-dms-upload",
  "version": "0.0.1-alpha.3",
  "description": "A document upload flow that can be implemented in any BMI React application. Documents are stored in DMS. Metadata can be added in the flow.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "scripts": {
    "prepare": "npm run build:ts:es && npm run build:ts && husky install",
    "start": "npm run storybook",
    "serve": "webpack serve --config webpack.dev.conf.js --open",
    "build": "webpack --config webpack.prod.conf.js",
    "build:ts": "tsc -b src/tsconfig.build.json",
    "build:ts:watch": "tsc -b src/tsconfig.build.json --watch",
    "build:ts:es": "tsc -b src/tsconfig.build.es.json",
    "build:ts:es:watch": "tsc -b src/tsconfig.build.es.json --watch",
    "build:storybook": "build-storybook",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "npm-run-all --parallel lint:js lint:css lint:types",
    "lint:js": "eslint --ignore-path .eslintignore -c .eslintrc.js \"src/**/*.{ts,tsx,js,jsx}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
    "lint:css": "stylelint --config-basedir ./ --ignore-path .stylelintignore --config .stylelintrc.js \"src/**/*.{tsx,ts}\"",
    "lint:types": "tsc --noEmit",
    "fix": "npm-run-all --parallel fix:js fix:css",
    "fix:js": "eslint --fix --ignore-path .eslintignore -c .eslintrc.js \"src/**/*.{ts,tsx,js,jsx}\" \"tests/**/*.{ts,tsx,js,jsx}\"",
    "ci:test": "jest --ci --reporters=default",
    "ci:eslint": "eslint --ignore-path .eslintignore -c .eslintrc.js \"src/**/*.{ts,tsx,js,jsx}\" \"tests/**/*.{ts,tsx,js,jsx}\" --format junit --output-file reports/eslint.xml",
    "ci:stylelint": "npm run stylelint -- --quiet --custom-formatter node_modules/stylelint-junit-formatter > reports/stylelint.xml",
    "ci:lint": "npm-run-all --parallel --silent --continue-on-error ci:eslint ci:stylelint",
    "storybook": "start-storybook -p 6006",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Amsterdam/bmi-dms-upload.git"
  },
  "keywords": [
    "dms",
    "upload",
    "bmi",
    "asc",
    "react",
    "component",
    "material-ui"
  ],
  "author": "Gemeente Amsterdam",
  "license": "MPL-2.0",
  "devDependencies": {
    "@babel/cli": "^7.13.14",
    "@babel/core": "^7.13.14",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-decorators": "^7.13.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
    "@babel/plugin-proposal-function-sent": "^7.12.13",
    "@babel/plugin-proposal-json-strings": "^7.13.8",
    "@babel/plugin-proposal-numeric-separator": "^7.12.13",
    "@babel/plugin-proposal-throw-expressions": "^7.12.13",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-import-meta": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.13.10",
    "@babel/preset-env": "^7.14.2",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@babel/runtime": "^7.14.0",
    "@storybook/addon-a11y": "^6.2.9",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-essentials": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.2.1",
    "@types/history": "^4.7.9",
    "@types/jest": "^26.0.23",
    "@types/react-dom": "^17.0.5",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.3.1",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.3",
    "@types/redux-saga": "^0.10.5",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-remove-properties": "^0.3.0",
    "babel-plugin-styled-components": "^1.12.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-webpack-alias": "^2.1.2",
    "clean-webpack-plugin": "^4.0.0",
    "commitizen": "^4.2.4",
    "css-loader": "^3.6.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.23.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-react": "^7.23.2",
    "handlebars": "^4.7.7",
    "handlebars-loader": "^1.7.1",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-styled-components": "^7.0.4",
    "jest-svg-transformer": "^1.0.0",
    "lint-staged": "^10.5.4",
    "mini-css-extract-plugin": "^1.6.0",
    "npm-run-all": "^4.1.5",
    "optimize-css-assets-webpack-plugin": "^6.0.0",
    "prettier": "^2.2.1",
    "react-router-dom": "^5.3.0",
    "standard-version": "^9.3.0",
    "style-loader": "^1.3.0",
    "stylelint": "^13.13.1",
    "stylelint-cli": "^1.3.0",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-junit-formatter": "^0.2.2",
    "stylelint-processor-styled-components": "^1.10.0",
    "ts-jest": "^26.5.4",
    "ts-loader": "8.2.0",
    "typescript": "^4.2.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@amsterdam/asc-assets": "^0.29.3",
    "@amsterdam/asc-ui": "^0.29.3",
    "@amsterdam/bmi-component-library": "0.0.5-alpha.4",
    "@material-ui/core": "^4.11.4",
    "@reduxjs/toolkit": "^1.6.2",
    "polished": "^4.1.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.5",
    "redux": "^4.1.1",
    "redux-devtools-extension": "^2.13.9",
    "redux-mock-store": "^1.5.4",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0",
    "styled-components": "^5.2.3",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "react": ">=17.0.2",
    "react-dom": ">=17.0.2",
    "react-redux": "^7.2.5",
    "styled-components": ">=5.2.3"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write --parser \"typescript\" --loglevel error",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
